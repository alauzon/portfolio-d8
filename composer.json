{
    "name": "acquia/blt-project",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0-only",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "autoload": {
        "psr-4": {
            "Example\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "require": {
        "php": ">=7.1",
        "acquia/blt": "^10.0.0",
        "acquia/drupal-spec-tool": "^2.0.0",
        "acquia/lightning": "^3.1.0",
        "cweagans/composer-patches": "^1.6",
        "drupal/acquia_connector": "^1.5.0",
        "drupal/acquia_purge": "^1.0-beta3",
        "drupal/admin_menu_toolbar": "3.x-dev",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.4",
        "drupal/captcha": "^1.0@beta",
        "drupal/coffee": "^1.0@beta",
        "drupal/cog": "^1.0.0",
        "drupal/computed_field": "^2.0@beta",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/config_split": "^1.0.0",
        "drupal/context": "^4.0@beta",
        "drupal/context_ui": "^4.0@beta",
        "drupal/core": "^8.6.0",
        "drupal/devel": "^2.0.0",
        "drupal/ds": "^3.3",
        "drupal/features": "^3.8.0",
        "drupal/field_collection": "^1.0@alpha",
        "drupal/field_group": "^1.0",
        "drupal/fontyourface": "^3.2",
        "drupal/hacked": "^2.0@beta",
        "drupal/htmlmail": "^3.0@alpha",
        "drupal/imagemagick": "^2.4",
        "drupal/isotope": "1.x-dev",
        "drupal/local_fonts": "^3.2",
        "drupal/mailsystem": "^4.1",
        "drupal/mason": "^1.0@beta",
        "drupal/memcache": "2.0-alpha7",
        "drupal/metatag": "^1.8",
        "drupal/metatag_facebook": "^1.8",
        "drupal/module_filter": "^3.1",
        "drupal/multiselect": "^1.0",
        "drupal/pathauto": "^1.4",
        "drupal/qa_accounts": "^1.0.0-alpha1",
        "drupal/recaptcha": "^2.4",
        "drupal/redirect": "^1.3",
        "drupal/rules": "^3.0@alpha",
        "drupal/rules_scheduler": "1.x-dev",
        "drupal/seckit": "^1.0.0-alpha2",
        "drupal/shield": "^1.2.0",
        "drupal/styleguide": "^1.0@alpha",
        "drupal/taxonomy_menu": "^3.4",
        "drupal/title": "2.x-dev",
        "drupal/token": "^1.5",
        "drupal/typekit_api": "^3.2",
        "drupal/upgrade_status": "^1.0@alpha",
        "drupal/views_bulk_operations": "^2.5",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drupal/xmlsitemap_custom": "^1.0@alpha",
        "drupal/xmlsitemap_engines": "^1.0@alpha"
    },
    "require-dev": {
        "acquia/blt-require-dev": "^10.0.0-alpha1",
        "geerlingguy/drupal-vm": "^5.0"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/admin_menu": {
                "Undefined function drupal_http_query": "https://www.drupal.org/files/issues/2019-01-29/2284015-8.patch"
            },
            "drupal/core":{
                "Error: Call to a member function getLabel() after enable layout_builder": "https://www.drupal.org/files/issues/2018-07-16/2985882-entityfield-2.patch"
            }
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "docroot/libraries/{$name}": ["type:drupal-library", "type:bower-asset", "type:npm-asset"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            }
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]
    }
}
